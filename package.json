{
  "name": "broom-console",
  "displayName": "Broom Console",
  "description": "This is a Visual Studio Code extension that allows you to delete all console logs and console debug output from a file.",
  "version": "1.0.4",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "Mathiew82",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mathiew82/broom-console"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "removeConsoleLogAndDebug.remove",
        "title": "%removeConsoleLogAndDebug.title%"
      },
      {
        "command": "removeAllConsole.remove",
        "title": "%removeAllConsole.title%"
      }
    ],
    "keybindings": [
      {
        "command": "removeConsoleLogAndDebug.remove",
        "key": "alt+shift+c",
        "when": "editorTextFocus"
      },
      {
        "command": "removeAllConsole.remove",
        "key": "alt+shift+x",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
